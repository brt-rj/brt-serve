  <path d="M 200,300 Q 500,250 800,300 T 1200,300" fill="none" stroke="#F4F3EF" stroke-width="10"/>
  
  <!-- Geometric anchor points -->
  <circle cx="300" cy="200" r="40" fill="#E1AD01"/>
  <circle cx="600" cy="200" r="35" fill="#F4F3EF"/>
