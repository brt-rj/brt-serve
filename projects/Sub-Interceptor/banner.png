<svg width="1200" height="400" xmlns="http://www.w3.org/2000/svg">
  <!-- Sub-Interceptor: Network/data interception theme -->
  <rect width="1200" height="400" fill="#1A1C18"/>
  
  <!-- Network nodes -->
  <circle cx="200" cy="200" r="60" fill="#E1AD01"/>
  <circle cx="450" cy="120" r="50" fill="none" stroke="#E1AD01" stroke-width="12"/>
  <circle cx="650" cy="280" r="55" fill="none" stroke="#F4F3EF" stroke-width="10"/>
  <circle cx="900" cy="180" r="65" fill="#E1AD01"/>
  <circle cx="1050" cy="100" r="45" fill="none" stroke="#E1AD01" stroke-width="10"/>
  
  <!-- Connection lines showing interception -->
  <line x1="200" y1="200" x2="450" y2="120" stroke="#F4F3EF" stroke-width="8"/>
  <line x1="450" y1="120" x2="650" y2="280" stroke="#E1AD01" stroke-width="8"/>
  <line x1="650" y1="280" x2="900" y2="180" stroke="#F4F3EF" stroke-width="8"/>
  <line x1="900" y1="180" x2="1050" y2="100" stroke="#E1AD01" stroke-width="8"/>
  <line x1="200" y1="200" x2="900" y2="180" stroke="#F4F3EF" stroke-width="6" opacity="0.5"/>
  
  <!-- Interceptor square in center -->
  <rect x="550" y="150" width="100" height="100" fill="#F4F3EF" stroke="#E1AD01" stroke-width="10"/>
  <rect x="575" y="175" width="50" height="50" fill="#E1AD01"/>
</svg>
