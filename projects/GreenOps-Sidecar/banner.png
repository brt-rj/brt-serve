  <!-- Pipeline stages -->
  <rect x="50" y="150" width="150" height="100" fill="#E1AD01" stroke="#F4F3EF" stroke-width="10"/>
  <polygon points="220,200 260,180 260,220" fill="#F4F3EF"/>
  
  <rect x="280" y="150" width="150" height="100" fill="none" stroke="#E1AD01" stroke-width="12"/>
  <polygon points="450,200 490,180 490,220" fill="#E1AD01"/>
  
  <rect x="510" y="150" width="150" height="100" fill="#F4F3EF"/>
  <polygon points="680,200 720,180 720,220" fill="#E1AD01"/>
  
  <rect x="740" y="150" width="150" height="100" fill="#E1AD01"/>
  <polygon points="910,200 950,180 950,220" fill="#F4F3EF"/>
  
  <rect x="970" y="150" width="180" height="100" fill="none" stroke="#F4F3EF" stroke-width="10"/>
  
  <!-- Sidecar indicators -->
  <circle cx="125" cy="80" r="35" fill="none" stroke="#E1AD01" stroke-width="10"/>
  <circle cx="355" cy="320" r="35" fill="#E1AD01"/>
  <circle cx="585" cy="80" r="35" fill="none" stroke="#F4F3EF" stroke-width="10"/>
  <circle cx="815" cy="320" r="35" fill="#F4F3EF"/>
  <circle cx="1060" cy="80" r="35" fill="none" stroke="#E1AD01" stroke-width="10"/>
  
