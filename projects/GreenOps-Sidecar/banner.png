  <line x1="125" y1="115" x2="125" y2="150" stroke="#E1AD01" stroke-width="6"/>
  <line x1="355" y1="250" x2="355" y2="285" stroke="#E1AD01" stroke-width="6"/>
  <line x1="585" y1="115" x2="585" y2="150" stroke="#F4F3EF" stroke-width="6"/>
  <line x1="815" y1="250" x2="815" y2="285" stroke="#F4F3EF" stroke-width="6"/>
  <line x1="1060" y1="115" x2="1060" y2="150" stroke="#E1AD01" stroke-width="6"/>
</svg>
